  "author": "Bogdan Begovic <bogdanbegovic@gmail.com>",
  "license": "MIT",
  "typings": "src/@types",
  "bin": {
    "codeowners-editor": "./dist/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './src/**/*' -c .eslintrc.json",
    "format": "prettier --write 'src/**/*.*(js|jsx|json|ts|tsx|css)'",
    "lint:fix": "eslint './src/**/*' --fix -c .eslintrc.json",
    "start": "concurrently \"yarn start:ui\" \"yarn start:core\"",
    "build": "yarn build:ui && yarn build:core",
    "start:core": "NODE_ENV=dev nodemon",
    "build:core": "tsc -p .",
    "start:ui": "webpack-dev-server --config webpack/webpack.config.js",
    "build:ui": "webpack --config webpack/webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
