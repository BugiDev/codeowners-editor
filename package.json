    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/react-select": "^2.0.14",
    "carlo": "^0.9.43",
    "clean-webpack-plugin": "^2.0.0",
    "prop-types": "^15.7.2",
    "rc-tree": "^1.15.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-icons": "^3.5.0",
    "react-redux": "^6.0.1",
    "react-select": "^2.4.1",
    "redux": "^4.0.1",
