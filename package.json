    "@types/node": "^11.11.0",
    "@types/react": "16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.3",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "concurrently": "^4.1.0",
